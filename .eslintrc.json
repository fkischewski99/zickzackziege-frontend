{
	"root": true,
	"env": {
		"node": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"sourceType": "module",
		"ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
		}
    },
    "plugins": ["react"],	
    "rules": {
		"no-console": 0,
		"eqeqeq":"warn",
		"no-cond-assign": 0,
		"no-unused-vars": 1,
		"no-extra-semi": "warn",
		"semi": "warn"
	}
}